class Positive {
  public static void main(String[] args) {
    int c=55;
        if(c > 0)
        {
            System.out.println("positive");
        }
        else if(c < 0)
        {
            System.out.println("negative");
        }
        else
        {
            System.out.println("zero");
        }
    }
}
